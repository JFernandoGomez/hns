{
  "entrypoint": "index.html",
  "shell": "src/hns-app/hns-app.html",
  "fragments": [
      "src/hns-app/hns-home.html",
      "src/hns-app/hns-gallery.html",
      "src/hns-app/hns-contactus.html",
      "src/hns-app/hns-contact.html",
      "src/hns-app/hns-about.html",
      "src/hns-app/custom-gallery.html"
  ],
  "lint": {
    "rules": [ "polymer-2" ]
  },
  "sources": [
    "src/**/*",
    "images/**/*",
    "assets/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"  
    }
  ]
}
